      </tuv>
    </tu>
    <tu>
      <tuv lang="en-US">
        <seg>A Creeping Calamity</seg>
      </tuv>
      <tuv lang="ru-RU" changeid="Yaroslav" changedate="20230326T164706Z" creationid="Yaroslav" creationdate="20230326T164641Z">
        <seg>Крадущаяся беда</seg>
      </tuv>
    </tu>
    <tu>
